{
	"usage": {
		"title": "*[❗] - Como usar?*",
		"args": "*[💡] - Tipos de parâmetros:*\n<obrigatório>\n[opcional]",
		"examples": "*[💯] - Exemplos:*"
	},
	"choose": {
		"usage": "<opção 1>, <opção 2>, [opção 3], [...]",
		"desc": "Escolha isso ou aquilo aleatoriamente, ou não...",
		"examples": [
			"Pizza, bolo, pudim, maçã",
			"Eu deveria fazer isso, eu não deveria fazer isso",
			"Bolacha, biscoito"
		],
		"result": "*[🏆] - Opção escolhida:* {{chosen}}"
	},
	"download": {
		"usage": "<audio/video> <url>",
		"desc": "Baixa um vídeo ou um áudio de várias plataformas diferentes",
		"examples": [
			"audio https://music.youtube.com/watch?v=4QLvEIXlF6Q",
			"a https://youtu.be/4QLvEIXlF6Q",
			"video https://youtu.be/4QLvEIXlF6Q"
		],
		"true": "*[⌛] - Baixando o vídeo... Por favor aguarde um momento.*",
		"false": "*[⌛] - Baixando o áudio... Por favor aguarde um momento.*"
	},
	"help": {
		"usage": "[nome de um comando]",
		"desc": "Exibe esta lista ou detalha um comando",
		"examples": [
			"translate",
			"sticker"
		],
		"title": "*[📖] - Menu de ajuda*",
		"prefix": "[⚙️] Alterar prefixo: *{{prefix}}*",
		"lang": "[🌎] Alterar idioma: *{{lang}}*"
	},
	"language": {
		"usage": "<código do idioma>",
		"desc": "Altera o idioma do bot",
		"examples": [
			"pt",
			"en"
		],
		"changed": "[🌎] - Idioma alterado com sucesso para *{{lng}}*"
	},
	"prefix": {
		"usage": "<prefixo com até 3 dígitos>",
		"desc": "Altera o prefixo dos comandos",
		"examples": [
			".",
			"!"
		],
		"changed": "[✔️] - O seu novo prefixo é: *{{prefix}}*"
	},
	"ping": {
		"desc": "Calcula o delay do bot",
		"pinging": "Calculando o tempo de resposta..."
	},
	"sticker": {
		"usage": "<imagem/gif anexado> [nobg: remover o fundo]",
		"desc": "Cria um sticker com imagem/gif e ainda remove o fundo",
		"examples": [
			"nobg <imagem anexada>",
			"<gif anexado>"
		],
		"nobuffer": "[⚠️] - Imagem incompatível ou não encontrada"
	},
	"stext": {
		"usage": "<texto/frase/palavra>",
		"desc": "Cria um sticker de texto",
		"examples": [
			"Você não pode mudar o seu passado, mas pode estragar o seu futuro",
			"Nenhum obstáculo é grande para quem desiste"
		]
	},
	"translate": {
		"usage": "<código do idioma: en/pt/fr...> <texto/frase/palavra>",
		"desc": "Google Tradutor",
		"examples": [
			"fr Pussy",
			"es War is love"
		]
	}
}
