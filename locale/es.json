{
	"usage": {
		"title": "*[❗] - ¿Cómo utilizar?*",
		"args": "*[💡] - Tipos de parámetros:*\n<requerido>\n[opcional]",
		"examples": "*[💯] - Ejemplos:*"
	},
	"choose": {
		"usage": "<opción 1>, <opción 2>, [opción 3], [...]",
		"desc": "Elige esto o aquello al azar, o no...",
		"examples": [
			"Pizza, pastel, pudín, manzana.",
			"Debería hacer esto, no debería hacer esto",
			"galleta, galleta"
		],
		"result": "*[🏆] - Opción elegida:* {{chosen}}"
	},
	"download": {
		"usage": "<audio/vídeo> <url>",
		"desc": "Descargar un vídeo o audio desde varias plataformas diferentes",
		"examples": [
			"sonido https://music.youtube.com/watch?v=4QLvEIXlF6Q",
			"a https://youtu.be/4QLvEIXlF6Q",
			"vídeo https://youtu.be/4QLvEIXlF6Q"
		],
		"true": "*[⌛] - Descargando el vídeo... Espere un momento.*",
		"false": "*[⌛] - Descargando audio... Espere un momento.*"
	},
	"help": {
		"usage": "[nombre de un comando]",
		"desc": "Mostrar esta lista o detallar un comando",
		"examples": ["traducir", "pegatina"],
		"title": "*[📖] - Menú de ayuda*",
		"prefix": "[⚙️] Cambiar prefijo: *{{prefix}}*",
		"lang": "[🌎] Cambiar idioma: *{{lang}}*"
	},
	"language": {
		"usage": "<código de idioma>",
		"desc": "Cambiar el idioma del bot",
		"examples": ["PT", "es"],
		"changed": "[🌎] - El idioma se cambió correctamente a *{{lng}}*"
	},
	"prefix": {
		"usage": "<prefijo de hasta 3 dígitos>",
		"desc": "Cambiar prefijo de comando",
		"examples": [".", "!"],
		"changed": "[✔️] - Tu nuevo prefijo es: *{{prefix}}*"
	},
	"ping": {
		"desc": "Calcula el retraso del bot",
		"pinging": "Calculando el tiempo de respuesta..."
	},
	"sticker": {
		"usage": "<imagen/gif adjunto> [nobg: eliminar fondo]",
		"desc": "Crea una pegatina con imagen/gif e incluso elimina el fondo",
		"examples": ["nobg <imagen adjunta>", "<gif adjunto>"],
		"nobuffer": "[⚠️] - Imagen incompatible o no encontrada"
	},
	"stext": {
		"usage": "<texto/oración/palabra>",
		"desc": "Crear una etiqueta de texto",
		"examples": [
			"No puedes cambiar tu pasado, pero puedes arruinar tu futuro.",
			"Ningún obstáculo es demasiado grande para aquellos que se dan por vencidos."
		]
	},
	"translate": {
		"usage": "<código de idioma: en/pt/fr...> <texto/frase/palabra>",
		"desc": "traductor de Google",
		"examples": ["fr coño", "es La guerra es amor"]
	}
}
