generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id     String @id
  lang   String
  prefix String
  cmds   Int
}

model msgs {
  author  String @id
  count   Int
  groupId String
  group   groups @relation(fields: [groupId], references: [id])
}

model groups {
  id   String @id
  msgs msgs[]
}
